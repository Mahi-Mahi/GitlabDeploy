{
    "name": "mahimahi/gitlab-deploy",
    "description": "Helps automate the deployment of projects onto servers by utilising Gitlab web hooks",
    "license": "MIT",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": ">=5.4"
    },
    "keywords": [
        "laravel",
        "git",
        "gitlab",
        "deployment"
    ],
    "authors": [
        {
            "name": "Olivier Mourlevat",
            "email": "olivier.mourlevat@mahi-mahi.fr"
        }
    ],
    "autoload": {
        "psr-4": {
            "MahiMahi\\GitlabDeploy\\": "src"
        }
    },
    "minimum-stability": "stable",
    "extras": {
        "laravel": {
            "providers": [
                "MahiMahi\\GitlabDeploy\\GitlabDeployServiceProvider"
            ]
        }
    }
}

